ğŸ“„ Exemplos de Consultas (Opcional)

SQL

-- Top 10 produtos mais vendidos
SELECT oi.product_id, COUNT(oi.order_id) AS total_pedidos
FROM olist_order_items_dataset oi
GROUP BY oi.product_id
ORDER BY total_pedidos DESC
LIMIT 10;

-- Total de vendas por estado do cliente
SELECT c.customer_state, SUM(op.payment_value) AS total_vendas
FROM olist_customers_dataset c
JOIN olist_orders_dataset o ON c.customer_id = o.customer_id
JOIN olist_order_payments_dataset op ON o.order_id = op.order_id
GROUP BY c.customer_state
ORDER BY total_vendas DESC;

